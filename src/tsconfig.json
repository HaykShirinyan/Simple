{
  "compilerOptions": {
    "experimentalDecorators": true,
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es5",
    "module": "none",
    "outFile": "../dist/simple.js",
    "declaration": true,
    "lib": [
      "es5",
      "es2015",
      "dom"
    ]
  },
  "compileOnSave": true,
  "exclude": [
    "node_modules"
  ]
}
